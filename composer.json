{
    "name": "visanduma/nova-two-factor",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "visanduma/nova-google2fa": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Visanduma\\NovaTwoFactor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Visanduma\\NovaTwoFactor\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
